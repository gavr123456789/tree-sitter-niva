{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "niva",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_expression"
      }
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keywords"
        },
        {
          "type": "SYMBOL",
          "name": "_identifier_like"
        },
        {
          "type": "SYMBOL",
          "name": "control_flow"
        },
        {
          "type": "SYMBOL",
          "name": "operator"
        },
        {
          "type": "SYMBOL",
          "name": "_comment_like"
        },
        {
          "type": "SYMBOL",
          "name": "_number_like"
        },
        {
          "type": "SYMBOL",
          "name": "_string_like"
        },
        {
          "type": "SYMBOL",
          "name": "block"
        }
      ]
    },
    "_string_like": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "char"
        },
        {
          "type": "SYMBOL",
          "name": "multi_string"
        }
      ]
    },
    "_number_like": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "float"
        },
        {
          "type": "SYMBOL",
          "name": "double"
        }
      ]
    },
    "_comment_like": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "doc_comment"
        }
      ]
    },
    "_identifier_like": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "type_name"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "explicit_type"
        },
        {
          "type": "SYMBOL",
          "name": "keyword_send"
        },
        {
          "type": "SYMBOL",
          "name": "annotation"
        }
      ]
    },
    "type_name": {
      "type": "PATTERN",
      "value": "[A-Z][a-zA-Z_\\-]*"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z_][a-zA-Z_\\-]*"
    },
    "explicit_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "::"
        }
      ]
    },
    "keyword_send": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        }
      ]
    },
    "annotation": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "operator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "STRING",
          "value": ">="
        },
        {
          "type": "STRING",
          "value": "<="
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "STRING",
          "value": "+"
        },
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "STRING",
          "value": "/"
        },
        {
          "type": "STRING",
          "value": "=="
        },
        {
          "type": "STRING",
          "value": "!="
        },
        {
          "type": "STRING",
          "value": "%"
        },
        {
          "type": "STRING",
          "value": "."
        }
      ]
    },
    "control_flow": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "|"
        },
        {
          "type": "STRING",
          "value": "|=>"
        },
        {
          "type": "STRING",
          "value": "=>"
        },
        {
          "type": "STRING",
          "value": "|>"
        },
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "STRING",
          "value": "?"
        },
        {
          "type": "STRING",
          "value": "!"
        },
        {
          "type": "STRING",
          "value": "^"
        },
        {
          "type": "STRING",
          "value": "&&"
        },
        {
          "type": "STRING",
          "value": "||"
        }
      ]
    },
    "keywords": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "false"
        },
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "union"
        },
        {
          "type": "STRING",
          "value": "type"
        },
        {
          "type": "STRING",
          "value": "enum"
        },
        {
          "type": "STRING",
          "value": "this"
        },
        {
          "type": "STRING",
          "value": "mut"
        },
        {
          "type": "STRING",
          "value": "null"
        },
        {
          "type": "STRING",
          "value": "new"
        },
        {
          "type": "STRING",
          "value": "do"
        },
        {
          "type": "STRING",
          "value": "Bind"
        },
        {
          "type": "STRING",
          "value": "package"
        },
        {
          "type": "STRING",
          "value": "constructor"
        },
        {
          "type": "STRING",
          "value": "errordomain"
        },
        {
          "type": "STRING",
          "value": "builder"
        },
        {
          "type": "STRING",
          "value": "on"
        }
      ]
    },
    "block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "["
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_expression"
              }
            },
            {
              "type": "STRING",
              "value": "]"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "#{"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_expression"
              }
            },
            {
              "type": "STRING",
              "value": "}"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "{"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_expression"
              }
            },
            {
              "type": "STRING",
              "value": "}"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_expression"
              }
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "#("
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_expression"
              }
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "double": {
      "type": "PATTERN",
      "value": "\\d+\\.\\d+"
    },
    "float": {
      "type": "PATTERN",
      "value": "\\d+\\.\\d+f"
    },
    "string": {
      "type": "PATTERN",
      "value": "\".*\""
    },
    "char": {
      "type": "PATTERN",
      "value": "'.*'"
    },
    "multi_string": {
      "type": "PATTERN",
      "value": "\"\"\".*\"\"\""
    },
    "comment": {
      "type": "PATTERN",
      "value": "\\/\\/.*"
    },
    "doc_comment": {
      "type": "PATTERN",
      "value": "\\/\\/\\/.*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}